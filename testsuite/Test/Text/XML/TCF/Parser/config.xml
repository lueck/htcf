<?xml version="1.0" encoding="utf-8"?>
<config>
  <textRoot
      namespace="http://www.tei-c.org/ns/1.0"
      name="text"/>
  <droppedTree>
    <simpleElement
	namespace="http://www.tei-c.org/ns/1.0"
	name="fw"/>
  </droppedTree>
  <tokenizer>
    <linebreak
	namespace="http://www.tei-c.org/ns/1.0"
	name="lb"/>
    <linebreak
	namespace="http://www.tei-c.org/ns/1.0"
	name="l"/>
    <preNumAbbrev
	string="Bar"/>
    <preNumAbbrev
	string="Dan"/>
    <hyphen char="-"/>
    <hyphen char="¬"/><!-- DTA uses not as hyphen -->
    <abbrev1Char abbrev="True"/>
    <month>Januar</month>
    <month>Jänner</month>
    <month>Jaenner</month>
    <month>Februar</month>
    <month>März</month>
    <month>April</month>
    <month>Mai</month>
    <month>Juni</month>
    <month>Juli</month>
    <month>July</month>
    <month>August</month>
    <month>September</month>
    <month>Oktober</month>
    <month>October</month>
    <month>November</month>
    <month>Dezember</month>
    <month>December</month>
  </tokenizer>
  <tcf>
    <tcfNamespace
	namespace="http://www.dspin.de/data/textcorpus"/>
    <idBase base="16"/>
    <idPrefixDelimiter delimiter="_"/>
  </tcf>
</config>
